table
  th
    td Name
    td Holes
    td Location
    td Par Total
    td Green Type
    td Edit
    td Delete
  each course,i in courses
    tr
      td=(i+1)
      td=course.course_name
      td=course.number_of_holes
      td=course.course_location
      td=course.par_total
      td=course.green_type
      td Edit
      td Delete